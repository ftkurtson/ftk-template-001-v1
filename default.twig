{% extends "layout.twig" %}

{% block content %}

<div class="bk-pt-default">

    <section class="bk-section">
        <header class="bk-header">
            <div class="header-wrapper clearfix">
               {{widget('feature', 'thisunqiuewidgetname', {"header": "profile", "strapline":"profile", "buttonText":"Lorem Ipsum","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '0','showBtn': true})|raw}}
        </header>
    </section>

    <div class="canvas">
        <div class="main wrapper clearfix">
            {{zone('main')|raw}}
        </div>
    </div>

    <footer class="bk-footer">
        <div class="wrapper clearfix">
            {{widget('companyaddress', 'companyaddress', {})|raw}}
        </div>
    </footer>

</div>

{% endblock %}
